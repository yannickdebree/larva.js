{
  "compilerOptions": {
    "baseUrl": "./src",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "module": "CommonJS",
    "noImplicitAny": false,
    "outDir": "./lib",
    "sourceMap": false,
    "strict": false,
    "strictFunctionTypes": false,
    "target": "ES5",
    "lib": [
      "es2015",
      "dom"
    ]
  }
}