{
  "files": [
    "./src/main.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": false,
    "module": "CommonJS",
    "noImplicitAny": false,
    "outDir": "dist",
    "sourceMap": true,
    "strict": false,
    "strictFunctionTypes": false,
    "target": "ES2015",
    "lib": [
      "es2015",
      "dom"
    ],
    "paths": {
      "@_components": [
        "src/components"
      ],
      "@_injectables": [
        "src/injectables"
      ],
      "@_kernel": [
        "src/kernel"
      ],
      "@_nodes": [
        "src/nodes"
      ],
      "@_shared": [
        "src/shared"
      ],
      "@_snake": [
        "src/snake"
      ],
    }
  }
}