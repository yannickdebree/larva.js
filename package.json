    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "http-server": "^0.11.1",
    "mocha": "^7.1.1",
    "prettier": "^1.19.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
